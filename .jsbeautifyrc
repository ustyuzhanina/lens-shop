{
  "beautify.language": {
    "indent_size": 2,
    "indent_char": " ",
    "max_preserve_newlines": 1,
    "indent_inner_html": true,
    "end_with_newline": true,
    "css": {
      "ext": ["css", "scss"]
      },
    "html": {
      "ext": ["htm", "html"],
      "indent_size": 4
    },
    "beautify.ignore": [
      "**/*.js",
      "node_modules"
    ]
  }
}
